{
  "compilerOptions": {
	"module": "system",
	"target": "es6",
	"sourceMap": true,
	"alwaysStrict": true,
	"allowJs": true,
	"checkJs": true,
	"charset": "utf8",
	"noImplicitAny": false,
	"noImplicitThis": false,
	"pretty": true,
	"removeComments": false,
	"diagnostics": true,
	"noEmitOnError": true,
	"experimentalDecorators": true,
	"emitDecoratorMetadata": true,
	"lib": ["es6", "dom"],
	"types": ["reflect-metadata"],
	"moduleResolution": "node"
  },
  "exclude": [
	"node_modules", "typings"
  ],
  "include": [
	"./**/*"
  ],
  "scripts": {
	"test": ""
  },
  "files": [
	"node_modules/systemjs/dist/system.js",
	"graphics/types.ts",
	"graphics/color.ts"
  ]
}